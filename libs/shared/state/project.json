{
  "name": "state",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/shared/state/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "codegen": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["graphql-codegen --config ./libs/shared/state/codegen.ts"],
        "parallel": false
      }
    },
    "build": {
      "executor": "@nx/rollup:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/shared/state",
        "tsConfig": "libs/shared/state/tsconfig.lib.json",
        "project": "libs/shared/state/package.json",
        "entryFile": "libs/shared/state/src/index.ts",
        "external": ["react", "react-dom", "react/jsx-runtime"],
        "rollupConfig": "@nx/react/plugins/bundle-rollup",
        "compiler": "babel",
        "assets": [
          {
            "glob": "libs/shared/state/README.md",
            "input": ".",
            "output": "."
          }
        ]
      }
    }
  }
}
